{
  "name": "dubhejs",
  "version": "1.0.2",
  "description": "js library",
  "main": "dist/dubhe.js",
  "scripts": {
    "test": "jest",
    "type-check": "tsc --noEmit",
    "type-check:watch": "npm run type-check -- --watch",
    "build": "npm run build:types && npm run build:js && npm run bundle",
